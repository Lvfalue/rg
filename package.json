{
  "name": "ts-node-react",
  "version": "1.0.0",
  "description": "",
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"webpack-dev-server --mode development --config ./client/config/webpack.dev.js\" \"npm run dev:egg\" ",
    "build": "webpack --config ./client/config/webpack.prod.js",
    "start:egg": "egg-scripts start --daemon --title=egg-server-server",
    "stop:egg": "egg-scripts stop --title=egg-server-server",
    "dev:egg": "egg-bin dev",
    "debug:egg": "egg-bin debug",
    "test-local:egg": "egg-bin test",
    "test:egg": "npm run lint:egg -- --fix && npm run test-local:egg",
    "cov:egg": "egg-bin cov",
    "tsc:egg": "ets && tsc -p tsconfig.json",
    "ci:egg": "npm run lint:egg && npm run cov:egg && npm run tsc:egg",
    "autod:egg": "autod",
    "lint:egg": "tslint --project . -c tslint.json",
    "clean:egg": "ets clean"
  },
  "keywords": [
    "koa",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
